<h2>Login</h2>

<form action="/auth/login" method="post">

<label for="">Email</label>
<input type="text" name="email" >

<label for="">Password</label>
<input type="password" name="password" >

<button type="submit">Login</button>
</form>

{{#if errorMessage}}
<ul>
    {{#each errorMessage}}
<li>{{this}}</li>

    {{/each}}
</ul>
{{/if}}